    "name": "GSA_RF_n100_dmy_dow_grps_shift_0",
    "model": {
        "module": "sklearn.ensemble",
        "model": "RandomForestRegressor",
        "kwargs": {
            "random_state":  0,
            "n_estimators": 100
        }
    },
    "inputs": [
        {
            "filename": "./data/US_2017_2019_daily_gsa.csv",
            "include": ["Date", "Total GSA"],
            "groupby": "Date", 
            "join_on": "Date"
        }, 
        {
            "filename": "./data/2017-Current OMD TV Spot Log-AP.csv",
            "filter": [{"by": "outlier", "op": "eq", "val": "No"}],
            "include": ["calendar_spotdate", "month", "year", "calendar_dow", "product", "grps"],
