            "split": [{"metric": "grps", "by": "product"}],
            "groupby": "calendar_spotdate", 
            "join_on": "calendar_spotdate"
        }
    ],
    "polynomial_features": [["year", "grps"]],
    "drop_after_poly": ["year", "grps"],
    "test_size": 0.2,
    "target": "Total GSA",
    "shift_target_by": 0
